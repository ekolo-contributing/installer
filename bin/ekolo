#!/usr/bin/env php
#!/usr/bin/env php
<?php
	/*
	|--------------------------------------------------------------------------
	| Include files
	|--------------------------------------------------------------------------
	|
	| Ce fichier inclue le fichier autoload de composer
	|
	*/
	if (file_exists(__DIR__.'/../../../autoload.php')) {
		require __DIR__.'/../../../autoload.php';
	} else {
		require __DIR__.'/../vendor/autoload.php';
	}

	(new Ekolo\Component\Installer\Installer)->run();